{
  "name": "is-animated-ts",
  "version": "2.0.2",
  "description": "Detects animated images",
  "keywords": [
    "animated",
    "animated gif",
    "animated image",
    "animated png",
    "animated webp",
    "animation",
    "apng",
    "gif",
    "png",
    "webp",
    "typescript"
  ],
  "homepage": "https://github.com/Iucasmaia/is-animated",
  "bugs": {
    "url": "https://github.com/Iucasmaia/is-animated/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Iucasmaia/is-animated.git"
  },
  "license": "MIT",
  "author": "Józef Sokołowski <j.k.sokolowski@gmail.com>",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist/lib"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "lint": "eslint lib/**/*.ts test/**/*.ts",
    "lint:fix": "eslint lib/**/*.ts test/**/*.ts --fix",
    "test": "yarn build && tape dist/test/*.js | tap-spec",
    "test-ci": "yarn build && nyc --reporter=lcov tape dist/test/*.js | tap-spec",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "@types/tape": "^5.8.1",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "nyc": "^17.0.0",
    "standard": "^15.0.1",
    "standard-version": "^9.3.2",
    "tap-spec": "^2.2.2",
    "tape": "^4.0.0",
    "typescript": "^5.9.3"
  }
}
